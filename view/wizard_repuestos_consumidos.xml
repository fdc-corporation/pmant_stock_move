<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="wizard_repuesto_consumido_view_form" model="ir.ui.view">
        <field name="name">wizard.repuesto.consumido.view.form</field>
        <field name="model">wizard.repuesto.consumido</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="tarea_id" invisible="1"></field>
                        <field name="requerimiento_id" invisible="1"></field>
                        <field name="cantidad_consumida"></field>
                    </group>
                </sheet>
                <footer>
                    <button string="Confirmar" type="object" name="action_confirmar_consumo" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>
